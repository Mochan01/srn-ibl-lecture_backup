              },
              {
                  "progress": {
                      "slide": 1,
                      "step": 7
                  },
                  "narrative": {
                      "narration": "よろしくね！"
                  },
                  "audio": {
                      "mp3": "ST_L1U1_07.mp3",
                      "time": 2000
